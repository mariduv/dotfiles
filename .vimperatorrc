set complete=sl

style browser .tab-close-button{ display: none; }

command! reader execute "open about:reader?url=" + content.location.href

nmap <F1> <Nop>
nmap <F4> :noh<CR>

nmap q <Nop>
nmap s tgoogle 
nmap S :winopen -private google 

nmap J gt
nmap K gT

nmap <C-r> :reader<CR>

source! ~/.vimperatorrc.local

" vim: set ft=vimperator:
