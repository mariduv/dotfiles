set complete=sl

style browser .tab-close-button{ display: none; }

command! reader execute "open about:reader?url=" + content.location.href

nmap <F1> <Nop>
nmap <F4> :noh<CR>

nmap q <Nop>
nmap s tddg 
nmap S :winopen -private ddg 

nmap J gt
nmap K gT

nmap <C-r> :reader<CR>

javascript <<EOJ
  (function() {
    var ddg = Services.search.getEngineByName('DuckDuckGo');
    if (ddg) ddg.alias = 'ddg';
  })();
EOJ

source! ~/.vimperatorrc.local

" vim: set ft=vimperator:
