script_line1 = use strict; use warnings; use v5.14; use experimentals;

[Colors]
[DataPrinter]
[FancyPrompt]
[Interrupt]

[Hints]
[Packages]
[LexicalPersistence]
[ResultCache]
[LoadClass]

